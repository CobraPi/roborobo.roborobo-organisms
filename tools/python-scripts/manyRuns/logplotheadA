set key box 
set key below
set pointsize 1

#set boxwidth 0.7 absolute
set boxwidth 1.0 absolute

set yrange [ -5 : 105 ] noreverse nowriteback


set datafile separator "," 
set title "active agents per generation"

set label 1 "#active agents" center rotate by 90 at graph 0, graph 0.5 offset -5

set xlabel "generations"

set term postscript eps color "Times-Roman" 20 enhanced
